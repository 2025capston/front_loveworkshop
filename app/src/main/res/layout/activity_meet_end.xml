<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".yes_or_no">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="355dp"
        android:layout_height="444dp"
        android:layout_marginTop="128dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/whitebox" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:fontFamily="@font/scd5"
        android:lineSpacingExtra="15dp"
        android:text="만남이 종료되었습니까?"
        android:textAlignment="center"
        android:textSize="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView18" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/inginging"
        android:layout_width="156dp"
        android:layout_height="55dp"
        android:layout_marginStart="96dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/yes_btn"
        android:fontFamily="@font/scd5"

        android:text="YES"
        android:textSize="24dp"
        app:layout_constraintStart_toStartOf="@+id/imageView18"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/idontlikehim"
        android:layout_width="156dp"
        android:layout_height="55dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="100dp"
        android:background="@drawable/no_btn"
        android:fontFamily="@font/scd5"
        android:text="NOT YET"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView18"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />
</androidx.constraintlayout.widget.ConstraintLayout>